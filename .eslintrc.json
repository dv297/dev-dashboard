{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "max-len": ["warn", 120],
    "no-use-before-define": "off",
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  }
}
